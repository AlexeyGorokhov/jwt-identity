{
  "name": "jwt-identity",
  "version": "3.0.0",
  "description": "JSON Web Token Authentication Middleware for Express.js",
  "main": "index.js",
  "engines": {
    "node": ">=8.0.0"
  },
  "scripts": {
    "test": "./node_modules/.bin/semistandard --verbose | ./node_modules/.bin/snazzy && ./node_modules/.bin/tape 'test/unit/**/*.test.js' | ./node_modules/.bin/tap-summary",
    "test:border": "./node_modules/.bin/tape 'test/border/**/*.test.js' | ./node_modules/.bin/tap-summary",
    "test:integration": "bash ./test/integration/run.sh"
  },
  "author": {
    "name": "Alexey Gorokhov",
    "email": "alexey.gorokhov@icloud.com",
    "url": ""
  },
  "license": "MIT",
  "devDependencies": {
    "body-parser": "^1.15.0",
    "express": "^4.13.4",
    "lodash": "^4.17.5",
    "proxyquire": "^1.7.4",
    "request": "^2.83.0",
    "request-promise-native": "^1.0.5",
    "semistandard": "^12.0.0",
    "sinon": "^4.3.0",
    "snazzy": "^7.0.0",
    "tap-summary": "^4.0.0",
    "tape": "^4.4.0"
  },
  "dependencies": {
    "jsonwebtoken": "^8.1.1"
  },
  "keywords": [
    "JSON Web Token",
    "Express middleware",
    "authorization",
    "authentication",
    "user management"
  ],
  "homepage": "https://github.com/AlexeyGorokhov/jwt-identity",
  "repository": {
    "type": "git",
    "url": "https://github.com/AlexeyGorokhov/jwt-identity"
  },
  "bugs": {
    "url": "https://github.com/AlexeyGorokhov/jwt-identity/issues"
  }
}
